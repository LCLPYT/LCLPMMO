{
  "schemaVersion": 1,
  "id": "lclpmmo",
  "version": "${version}",

  "name": "LCLPMMO",
  "description": "A mod that adds MMO RPG content to Minecraft. Intended to be used on LCLPServer 5.0",
  "authors": [
    "LCLP",
    "DerBerliner"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/LCLPMMO",
    "sources": "https://github.com/LCLPYT/LCLPMMO"
  },

  "license": "MIT",
  "icon": "assets/lclpmmo/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "work.lclpnet.mmo.LCLPMMO"
    ],
    "client": [
      "work.lclpnet.mmo.client.LCLPMMOClient"
    ]
  },
  "mixins": [
    "lclpmmo.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x",
    "mmocontent": "1.2.x"
  },
  "suggests": {},
  "accessWidener": "lclpmmo.accesswidener"
}
