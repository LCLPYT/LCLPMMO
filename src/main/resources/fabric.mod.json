{
  "schemaVersion": 1,
  "id": "lclpmmo",
  "version": "${version}",
  "name": "LCLPMMO",
  "description": "A mod that adds MMO RPG content to Minecraft. Intended to be used on LCLPServer 5.0",
  "authors": [
    "LCLP",
    "DerBerliner"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/LCLPMMO",
    "sources": "https://github.com/LCLPYT/LCLPMMO"
  },
  "license": "MIT",
  "icon": "assets/lclpmmo/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "work.lclpnet.mmo.LCLPMMO"
    ],
    "client": [
      "work.lclpnet.mmo.client.LCLPMMOClient"
    ],
    "server": [
      "work.lclpnet.mmo.server.LCLPMMOServer"
    ]
  },
  "mixins": [
    "lclpmmo.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": "1.18.2",
    "mmocontent": "1.12.x",
    "quark": "1.6.x",
    "cfm": "1.4.x",
    "geckolib3": ">=3.0.60",
    "java": ">=17"
  },
  "suggests": {},
  "accessWidener": "lclpmmo.accesswidener"
}
